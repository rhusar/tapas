<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/2.1.1/distro-source/core/src/gwt-module.dtd">
<module>
   <!-- Inherit the core Web Toolkit stuff. -->
   <inherits name="com.google.gwt.user.User"/>

   <!--  inherit css based theme -->
   <inherits name="com.google.gwt.user.theme.standard.Standard"/>

   <!-- Specify the app entry point class.                         -->
   <entry-point class="com.javacodegeeks.war.client.Application"/>

   <!-- Specify the application specific style sheet.              -->
   <stylesheet src="Application.css" />
      
   <source path="client"/>
   <source path="shared"/>

   <!-- At development time, optimize only for 1 browser -->
   <!-- Firefox: <set-property name="user.agent" value="gecko1_8" /> -->
<!--   <set-property name="user.agent" value="safari" />
   <set-property name="locale" value="cs" />-->
</module>
